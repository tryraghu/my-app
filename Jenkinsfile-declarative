currentBuild.displayName = "online-shoping#"+currentBuild.number

pipeline{
    agent any
    environment{
       PATH = "/srv/distro/maven-3/bin:$PATH" 
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/tryraghu/my-app.git'
               }
            }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
               
            }
        }
        
        }
    }
    
