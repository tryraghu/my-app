pipeline{
    agent any
    enviroment{
       PATH = "/srv/distro/maven-3/bin:$PATH" 
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/tryraghu/my-app.git'
               }
            }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
    
}
